{
    "version": 2,
    "builds": [
        {
            "src": "dist/portfolio-alejo-amaris/server/server.mjs",
            "use": "@vercel/node"
        },
        {
            "src": "dist/portfolio-alejo-amaris/browser/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/portfolio-alejo-amaris/server/server.mjs"
        }
    ]
}